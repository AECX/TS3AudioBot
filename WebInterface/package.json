{
  "name": "ts3abweb",
  "version": "0.1.0",
  "description": "Provides a ui to control the TS3AudioBot",
  "main": "script.js",
  "scripts": {
    "build:all": "npm run build:tsc && npm run build:less",
    "build:tsc": "tsc --build",
    "build:less": "lessc --source-map=out/styles.css.map less/styles.less out/styles.css",
    "tool:copy-webdata": "copyfiles -u 1 \"html/**/*\" build",
    "optimize:all": "npm run optimize:tsc && npm run optimize:less",
    "optimize:tsc": "uglifyjs out/script.js -c -o build/script.js",
    "optimize:less": "cleancss out/styles.css -o build/styles.css -O2",
    "watch:all": "parallelshell \"npm run watch:tsc\" \"npm run watch:less\"",
    "watch:tsc": "tsc --watch",
    "watch:less": "onchange \"less/**/*.less\" -- npm run build:less",
    "build:publish": "npm run build:all && npm run tool:copy-webdata && npm run optimize:all"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Splamy/TS3AudioBot"
  },
  "keywords": [
    "ts3",
    "ts3ab",
    "interface",
    "ui",
    "audio",
    "bot"
  ],
  "author": "Splamy",
  "license": "OSL-3.0",
  "devDependencies": {
    "clean-css-cli": "^4.2.1",
    "copyfiles": "^2.1.0",
    "del-cli": "^1.1.0",
    "less": "^3.8.1",
    "onchange": "^4.1.0",
    "parallelshell": "^3.0.2",
    "typescript": "^3.0.3",
    "uglify-es": "^3.3.9"
  }
}
